{
  "schemaVersion": 1,
  "id": "advancedgenetics",
  "version": "${version}",

  "name": "Advanced Genetics",
  "description": "Explore genetics by splicing your DNA with the mobs in your Minecraft world to gain their unique abilities, like shooting fireballs, eating grass, or climbing walls. There are dozens of abilities to discover!",
  "authors": [
    "TechnoVision",
    "Kirbocabana"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/advanced-genetics",
    "sources": "https://github.com/TechnoVisionDev/Advanced-Genetics"
  },

  "license": "MIT",
  "icon": "assets/advancedgenetics/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "com.technovision.advancedgenetics.AdvancedGenetics"
    ],
    "client": [
      "com.technovision.advancedgenetics.AdvancedGeneticsClient"
    ],
    "cardinal-components": [
      "com.technovision.advancedgenetics.registry.ComponentRegistry"
    ]
  },
  "mixins": [
    "advancedgenetics.mixins.json"
  ],
  "custom": {
    "cardinal-components": [
      "advancedgenetics:player_genetics"
    ]
  },

  "depends": {
    "fabricloader": ">=0.14.6",
    "fabric": "*",
    "minecraft": "~1.19",
    "java": ">=17"
  }
}
